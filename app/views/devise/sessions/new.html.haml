%h2 Log in
= form_with(model: resource, url: session_path(resource_name), id: 'new_user', class: 'new_user', local: true) do |f| 
  .field
    = f.label :email
    %br/
    = f.email_field :email, autofocus: true, autocomplete: "email"
  .field
    = f.label :password
    %br/
    = f.password_field :password, autocomplete: "current-password"
  - if devise_mapping.rememberable?
    .field
      = f.check_box :remember_me
      = f.label :remember_me
  .actions
    = f.submit "Log in"
%br/
= render "devise/shared/links"
